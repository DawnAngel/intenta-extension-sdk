{
  "name": "Intenta SDK",
  "version": "2.0.1",
  "manifest_version":2,
  "description": "Intenta CHROME SDK Demo",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs","cookies", "http://*/", "https://*/","webRequest", "webRequestBlocking"
  ],
  "background": {
    "scripts": [

      "intenta/dist/intenta_background.js",
      "app_background.js"
    ]
  },

  "content_scripts":[
  	{
  		"matches": [
  			"<all_urls>"
  		],
      "js": [
        "intenta/dist/intenta_content_script.js",
        "app_content_script.js"
      ]
  	}
  ]
}